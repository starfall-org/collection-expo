{
  "expo": {
    "name": "Douyin Collection",
    "slug": "douyin-collection",
    "version": "1.0.1",
    "orientation": "default",
    "icon": "./assets/movie.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/movie.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/movie.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["INTERNET"],
      "package": "com.expo.douyincollection"
    },
    "web": {
      "favicon": "./assets/movie.png"
    },
    "extra": {
      "eas": {
        "projectId": "05bb547e-98df-475b-95cd-551977cf3f3c"
      }
    },
    "owner": "tonglanhchua",
    "plugins": [
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ]
    ]
  }
}
