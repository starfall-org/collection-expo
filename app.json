{
  "expo": {
    "name": "Collection",
    "slug": "collection",
    "version": "1.2.0",
    "orientation": "default",
    "icon": "./assets/Icon.jpeg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/Splash.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/Icon.jpeg",
      "androidMode": "default"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.expo.collection"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Splash.jpeg",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "INTERNET",
        "NOTIFICATIONS",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "app.expo.collection"
    },
    "web": {
      "favicon": "./assets/Icon.jpeg"
    },
    "extra": {
      "eas": {
        "projectId": "05bb547e-98df-475b-95cd-551977cf3f3c"
      }
    },
    "owner": "tonglanhchua",
    "plugins": [
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ]
    ]
  }
}
